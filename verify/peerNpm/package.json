{
  "name": "@typefox/peer-check-npm",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "volta": {
    "node": "22.19.0",
    "npm": "10.9.3"
  },
  "dependencies": {
    "monaco-languageclient-examples": "~2025.9.2",
    "vscode": "npm:@codingame/monaco-vscode-extension-api@^21.2.1"
  },
  "devDependencies": {
    "typescript": "~5.9.2",
    "shx": "~0.4.0",
    "vite": "~7.1.6"
  },
  "scripts": {
    "verify": "npm install && npm run build && npm run start",
    "verify:ci": "npm install && npm run build",
    "clean": "shx rm -fr dist",
    "build:msg": "echo Building client-vite example:",
    "build:tsc": "tsc --build tsconfig.json",
    "build": "npm run build:msg && npm run clean && npm run build:tsc && npm run vite:build",
    "dev": "vite",
    "dev:debug": "vite --debug --force",
    "vite:build": "vite build",
    "start": "vite preview"
  }
}
