{
  "name": "monaco-languageclient-examples",
  "version": "8.1.1",
  "description": "Monaco Language client examples",
  "author": {
    "name": "TypeFox GmbH",
    "url": "http://www.typefox.io"
  },
  "homepage": "https://github.com/TypeFox/monaco-languageclient/blob/main/packages/examples/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TypeFox/monaco-languageclient.git",
    "directory": "packages/examples"
  },
  "bugs": {
    "url": "https://github.com/TypeFox/monaco-languageclient/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "default": "./dist/node.js"
    },
    "./json-client": {
      "types": "./dist/json/client/wrapperWs.d.ts",
      "default": "./dist/json/client/wrapperWs.js"
    },
    "./python-client": {
      "types": "./dist/python/client/main.d.ts",
      "default": "./dist/python/client/main.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index"
      ],
      "node": [
        "dist/node"
      ],
      "json-client": [
        "dist/json/client/wrapperWs"
      ],
      "python-client": [
        "dist/python/client/main"
      ]
    }
  },
  "dependencies": {
    "@codingame/monaco-vscode-configuration-service-override": "~3.2.3",
    "@codingame/monaco-vscode-editor-service-override": "~3.2.3",
    "@codingame/monaco-vscode-extensions-service-override": "~3.2.3",
    "@codingame/monaco-vscode-files-service-override": "~3.2.3",
    "@codingame/monaco-vscode-groovy-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-keybindings-service-override": "~3.2.3",
    "@codingame/monaco-vscode-javascript-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-json-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-python-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-standalone-languages": "~3.2.3",
    "@codingame/monaco-vscode-standalone-typescript-language-features": "~3.2.3",
    "@codingame/monaco-vscode-standalone-json-language-features": "~3.2.3",
    "@codingame/monaco-vscode-textmate-service-override": "~3.2.3",
    "@codingame/monaco-vscode-theme-defaults-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-theme-service-override": "~3.2.3",
    "@codingame/monaco-vscode-typescript-basics-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-typescript-language-features-default-extension": "~3.2.3",
    "@codingame/monaco-vscode-language-pack-de": "~3.2.3",
    "@codingame/monaco-vscode-language-pack-es": "~3.2.3",
    "@codingame/monaco-vscode-language-pack-fr": "~3.2.3",
    "@typefox/monaco-editor-react": "~3.0.2",
    "express": "~4.19.2",
    "langium": "3.0.0",
    "monaco-editor": "npm:@codingame/monaco-vscode-editor-api@~3.2.3",
    "monaco-editor-wrapper": "~4.0.2",
    "monaco-languageclient": "~8.1.1",
    "pyright": "~1.1.356",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "request-light": "~0.7.0",
    "vscode": "npm:@codingame/monaco-vscode-api@~3.2.3",
    "vscode-json-languageservice": "~5.3.10",
    "vscode-languageclient": "~9.0.1",
    "vscode-languageserver": "~9.0.1",
    "vscode-uri": "~3.0.8",
    "vscode-ws-jsonrpc": "~3.3.0",
    "ws": "~8.16.0"
  },
  "devDependencies": {
    "@types/express": "~4.17.21",
    "@types/ws": "~8.5.10",
    "langium-cli": "3.0.1",
    "ts-node": "~10.9.1",
    "vscode-languageserver-types": "~3.17.5"
  },
  "volta": {
    "node": "20.12.0",
    "npm": "10.5.0"
  },
  "files": [
    "dist",
    "src",
    "*.html",
    "*.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "shx rm -fr dist *.tsbuildinfo",
    "compile": "tsc --build tsconfig.src.json",
    "build:msg": "echo Building main examples:",
    "build": "npm run build:msg && npm run clean && npm run compile",
    "start:server:json": "node --loader ts-node/esm src/json/server/direct.ts",
    "start:server:python": "node --loader ts-node/esm src/python/server/direct.ts",
    "start:server:groovy": "node --watch --loader  ts-node/esm src/groovy/server/direct.ts",
    "langium:generate": "langium generate --file ./src/langium/statemachine/config/langium-config.json"
  }
}
